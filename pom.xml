<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.1.4.RELEASE</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>tn.esprit</groupId>
	<artifactId>ExamThourayaS2</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>ExamThourayaS2</name>
	<description>Demo project for Spring Boot</description>
	<Propriétés>
		<java.version>1.8</java.version>
		<maven-jar-plugin.version>3.1.1</maven-jar-plugin.version>
	</Propriétés>
	<distributionGestion>
<dépôt>
<id>deploymentRepo</id>
<nom>Référentiel Nexus</nom>
<url>http://localhost:8081/repository/maven-releases/</url>
</dépôt>
</distributionGestion>
	<Dépendances>
		<dépendance>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-jpa</artifactId>
		</dépendance>
		<dépendance>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
		</dépendance>

		<dépendance>
			<groupId>mysql</groupId>
			<artifactId>mysql-connector-java</artifactId>
			<portée>exécution</portée>
		</dépendance>
		<dépendance>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<facultatif>vrai</facultatif>
		</dépendance>
		<dépendance>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<portée>test</portée>
		</dépendance>

		<dépendance>
			<groupId>io.springfox</groupId>
			<artifactId>springfox-boot-starter</artifactId>
			<version>3.0.0</version>
		</dépendance>
		<dépendance>
			<groupId>io.springfox</groupId>
			<artifactId>springfox-swagger-ui</artifactId>
			<version>3.0.0</version>
		</dépendance>
		<dépendance>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-api</artifactId>
			<version>5.8.2</version>
			<portée>compiler</portée>
		</dépendance>
		<dépendance>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<portée>test</portée>
		</dépendance>
		<dépendance>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-test</artifactId>
			<version>2.7.1</version>
			<portée>compiler</portée>
		</dépendance>
		<dépendance>
			<groupId>org.testng</groupId>
			<artifactId>testng</artifactId>
			<version>RELEASE</version>
			<portée>test</portée>
		</dépendance>

	</Dépendances>

	<construire>
		<Plugins>
			<.plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<version>2.5.3</version>
				<configuration>
					<Exclut>
						<exclure>
							<groupId>org.projectlombok</groupId>
							<artifactId>lombok</artifactId>
						</exclure>
					</Exclut>
				</configuration>
			</.plugin>
			<.plugin>
				<groupId>org.jacoco</groupId>
				<artifactId>jacoco-maven-plugin</artifactId>
				<version>0.8.7</version>
				<Exécutions>
					<exécution>
						<buts>
							<but>prepare-agent</objectif>
						</buts>
					</exécution>
					<!-- attaché à la phase de test Maven -->
					<exécution>
						<id>rapport</id>
						<phase>test</phase>
						<buts>
							<objectif>rapport</objectif>
						</buts>
					</exécution>
				</Exécutions>
			</.plugin>
			<.plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>3.1.0</version>
			</.plugin>
			<.plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.19.1</version> <!-- compte jusqu’à présent-->
				<Dépendances>
					<!-- laisser surefire exécuter JUnit 4 mais aussi JUnit 5 tests -->
					<dépendance>
						<groupId>org.junit.platform</groupId>
						<artifactId>junit-platform-surefire-provider</artifactId>
						<version>1.0.1</version>
					</dépendance>
					<!-- Moteur vintage JUnit pour exécuter les tests JUnit 3 ou JUnit 4 -->
					<dépendance>
						<groupId>org.junit.vintage</groupId>
						<artifactId>junit-vintage-engine</artifactId>
						<version>4.12.1</version>
					</dépendance>
					<!-- Le moteur JUnit 5 pour exécuter les tests JUnit 5 -->
					<dépendance>
						<groupId>org.junit.jupiter</groupId>
						<artifactId>junit-jupiter-engine</artifactId>
						<version>5.0.1</version>
					</dépendance>
				</Dépendances>
			</.plugin>
		</Plugins>
	</construire>

</projet>
	
